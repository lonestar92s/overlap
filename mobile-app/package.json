{
  "name": "mobile-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install || true",
    "update:production": "eas update --branch production",
    "update:preview": "eas update --branch preview"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.0",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.1.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-navigation/bottom-tabs": "^7.4.3",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@rnmapbox/maps": "^10.1.40",
    "axios": "^1.10.0",
    "expo": "~54.0.0",
    "expo-dev-client": "~6.0.18",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-media-library": "~18.2.0",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.13",
    "expo-web-browser": "~15.0.9",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-autocomplete-input": "^5.5.6",
    "react-native-calendars": "^1.1313.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-image-crop-picker": "^0.51.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "react-dom": "19.1.0",
    "react-test-renderer": "19.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@rnmapbox/maps)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js",
      "@testing-library/jest-native/extend-expect"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/__tests__/**"
    ]
  },
  "private": true
}
